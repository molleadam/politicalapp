<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent" android:layout_width="fill_parent"
	android:orientation="vertical" style="@style/lista">





	<Gallery android:id="@+id/scrollgallery" android:gravity="center_vertical|center_horizontal"
		android:layout_gravity="center_vertical|center_horizontal"
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:unselectedAlpha="1"></Gallery>

	<RelativeLayout android:layout_width="fill_parent"
		android:gravity="center_vertical|center_horizontal"
		android:layout_height="45dip" android:layout_marginTop="-45dip"
		android:background="@drawable/map_toolbar_bg" android:paddingTop="2dip"
		android:id="@+id/RelativeLayoutBottom">
		<LinearLayout android:id="@+id/linearLayout1"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:orientation="horizontal">
			<Button android:text="@string/download" android:id="@+id/Button01"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_marginRight="3dip">
			</Button>
			<Button android:id="@+id/Button02" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="@string/set_wallpaper"
				android:layout_marginRight="3dip">
			</Button>
		</LinearLayout>
	</RelativeLayout>



</LinearLayout>
